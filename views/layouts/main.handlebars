          }
          footer {
            width: 100%;
            background-color: black;
            padding: 3%;
            color: #FFF;
          }
    
          @media (max-width: 600px) {
            .carousel-caption {
              display: none;
            }
            #icon {
              max-width: 150px;
            }
            h2 {
              font-size: 1.7em;
            }
          }
    
